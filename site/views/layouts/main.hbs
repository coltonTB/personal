<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width">
  <title>Colton Brown</title>
  <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700|Oswald' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" type="text/css" href="/normal.css">
  <link rel="stylesheet" type="text/css" href="/style.css">
  <style>
    .pdf-only{display: none;}
    .site-only{display: initial;}
  </style>

  <script type="text/javascript">
    function randOct(){
      return Math.floor(Math.random()*256);
    };
    function randColor(){
      return 'rgb('+randOct()+','+randOct()+','+randOct()+')';
    };
  </script>
  
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-93943838-1', 'auto');
    ga('send', 'pageview');

  </script>

</head>
<body>

  <canvas id="bg-canvas" width="500" height="500"></canvas>
 
  <div class="container">
    {{>navbar}}
    {{{body}}}

    <div class="footer">
      &copy; 2017 Colton Brown
    </div>

  </div>

  <script type="text/javascript">
    var selected = document.getElementsByClassName('nav-link-selected');
    Array.prototype.forEach.call(selected, function(el){
      el.setAttribute('style', 'border-color:'+randColor());
    });
  </script>

</body>
</html>
